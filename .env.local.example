HUAFENG_PROVIDER=deepseek-api

# DeepSeek 公网
HUAFENG_DEEPSEEK_BASE_URL=https://api.deepseek.com
HUAFENG_DEEPSEEK_API_KEY=你的公网APIKey

# 数据库（本地运行Python直连容器端口）
HUAFENG_LOCAL_POSTGRES_HOST=127.0.0.1
HUAFENG_LOCAL_POSTGRES_PORT=5432
HUAFENG_LOCAL_POSTGRES_USER=postgres
HUAFENG_LOCAL_POSTGRES_PASSWORD=postgres
HUAFENG_LOCAL_POSTGRES_DB=industrial_db
HUAFENG_LOCAL_POSTGRES_SSLMODE=disable

# 模型（可按需调整）
HUAFENG_TEXT2SQL_MODEL=deepseek-chat
HUAFENG_ANALYSIS_MODEL=deepseek-chat